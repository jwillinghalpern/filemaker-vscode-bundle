{
    "name": "filemaker-vscode",
    "displayName": "FileMaker VSCode",
    "description": "FileMaker syntax highlighting and snippets for Visual Studio Code",
    "version": "1.2.0",
    "publisher": "jwillinghalpern",
    "repository": {
        "type": "git",
        "url": "https://github.com/jwillinghalpern/filemaker-vscode-bundle"
    },
    "icon": "resources/filemaker-vscode-128.png",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "filemaker",
            "aliases": [
                "FileMaker",
                "Filemaker",
                "filemaker"
            ],
            "extensions": [
                ".fmfn",
                ".calc"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "filemaker",
            "scopeName": "source.filemaker",
            "path": "./syntaxes/FileMaker.tmLanguage"
        }],
        "snippets": [{
            "language": "filemaker",
            "path": "./snippets/filemaker.json"
        }]
    }
}